{
  "name": "codejedi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": "run-s install:**",
    "install:server": "cd backend && npm install",
    "install:app": "cd frontend && npm install",
    "start": "run-s start:**",
    "start:server": "cd backend && npm run start",
    "start:app": "cd frontend && npm run start",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "test:user": "cd backend && npm run test:user",
    "test:auth": "cd backend && npm run test:auth",
    "test:comment": "cd backend && npm run test:comment",
    "clusterBack": "NODE_ENV=production pm2 start backend/server.js -i 1 --name codejedi-backend",
    "clusterReloadBack": "NODE_ENV=production pm2 reload codejedi-backend",
    "clusterFront": "NODE_ENV=production && cd frontend && pm2 start --name codejedi-front node_modules/react-scripts/scripts/start.js --node-args=\"--openssl-legacy-provider\"",
    "clusterReloadFront": "NODE_ENV=production pm2 reload codejedi-front"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codefactory-dev/codejedi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codefactory-dev/codejedi/issues"
  },
  "homepage": "https://github.com/codefactory-dev/codejedi#readme",
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5"
  }
}
